{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "CosmosDb": {
    "Account": "YOUR_COSMOS_DB_ACCOUNT_URI", // e.g., https://your-cosmos-db.documents.azure.com:443/
    "Key": "YOUR_COSMOS_DB_PRIMARY_KEY",
    "DatabaseName": "YourDatabaseName",
    "ContainerName": "YourContainerName"
  },
  "UseLocalDataService": true // Set to true to use local in-memory data, false to use Azure Cosmos DB
}